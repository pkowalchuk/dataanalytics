import lasio
import pandas as pd
import math
import matplotlib.pyplot as plt


def read_las_files(file_list):
    dfs=pd.DataFrame()
    for file in file_list:
        well = lasio.read('Data/LAS/'+file)
        df = well.df()
        dfs=pd.concat([dfs, df], ignore_index=False)
    return dfs

