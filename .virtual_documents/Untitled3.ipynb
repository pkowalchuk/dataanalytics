BS = 17.5
Bit_Area = (BS**2)*math.pi/4
factor = 0.35

def calculate_MSE(row):
    return (factor / Bit_Area) * (row['TFBA'] + ( 120 * math.pi * row['RPMTOTAL'] * row['FTAA']) / (row['FRPI'] * 3.28084))


df2['MSE'] = df2.apply(calculate_MSE,axis=1)
df2 = df2.dropna()



